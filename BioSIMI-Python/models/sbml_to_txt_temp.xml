<model>

<reaction text="inP+X--inP_X" after="--">
    <delay type="none"/>
    <propensity type="general" rate="X*_k1f*inP - _k1r*inP_X" />
</reaction>

<reaction text="inP_X--inP+X_P" after="--">
    <delay type="none"/>
    <propensity type="general" rate="_k2f*inP_X" />
</reaction>

<reaction text="E+X_P--E_X_P" after="--">
    <delay type="none"/>
    <propensity type="general" rate="E*X_P*_k3f - E_X_P*_k3r" />
</reaction>

<reaction text="E_X_P--E+X" after="--">
    <delay type="none"/>
    <propensity type="general" rate="E_X_P*_k4f" />
</reaction>

<reaction text="inP+X_P--inP_X_P" after="--">
    <delay type="none"/>
    <propensity type="general" rate="X_P*_k5f*inP - _k5r*inP_X_P" />
</reaction>

<reaction text="inP_X_P--X_P_P+inP" after="--">
    <delay type="none"/>
    <propensity type="general" rate="_k6f*inP_X_P" />
</reaction>

<reaction text="E+X_P_P--E_X_P_P" after="--">
    <delay type="none"/>
    <propensity type="general" rate="E*X_P_P*10*heaviside(t-6) - E_X_P_P*_k7r" />
</reaction>

<reaction text="E_X_P_P--X_P+E" after="--">
    <delay type="none"/>
    <propensity type="general" rate="E_X_P_P*_k8f" />
</reaction>

<rule type="assignment" frequency="repeated" equation="inP=2*X" />

<species name="inP" value="5.000000000000000000E+01" />
<species name="X" value="5.000000000000000000E+01" />
<species name="inP_X" value="0.000000000000000000E+00" />
<species name="X_P" value="0.000000000000000000E+00" />
<species name="E" value="5.000000000000000000E+01" />
<species name="E_X_P" value="0.000000000000000000E+00" />
<species name="inP_X_P" value="0.000000000000000000E+00" />
<species name="X_P_P" value="0.000000000000000000E+00" />
<species name="E_X_P_P" value="0.000000000000000000E+00" />

<parameter name="k1f" value="1.000000000000000000E+00"/>
<parameter name="k1r" value="1.000000000000000000E+00"/>
<parameter name="k2f" value="1.000000000000000000E+00"/>
<parameter name="k3f" value="1.000000000000000000E+00"/>
<parameter name="k3r" value="1.000000000000000000E+00"/>
<parameter name="k4f" value="1.000000000000000000E+00"/>
<parameter name="k5f" value="1.000000000000000000E+00"/>
<parameter name="k5r" value="1.000000000000000000E+00"/>
<parameter name="k6f" value="1.000000000000000000E+00"/>
<parameter name="k7f" value="1.000000000000000000E+00"/>
<parameter name="k7r" value="1.000000000000000000E+00"/>
<parameter name="k8f" value="1.000000000000000000E+00"/>

</model>
